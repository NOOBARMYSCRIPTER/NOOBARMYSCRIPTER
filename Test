if not game.CoreGui:FindFirstChild("NoobArmyText") then
local ScreenGui = Instance.new("ScreenGui")
local TextButton = Instance.new("TextButton")
ScreenGui.Name = "NoobArmyText"
ScreenGui.Parent = game.CoreGui
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
TextButton.Parent = ScreenGui
TextButton.BackgroundColor3 = Color3.fromRGB(196, 40, 28)
TextButton.Position = UDim2.new(0.44, 0, 0.1, 0)
TextButton.Size = UDim2.new(0, 320, 0, 50)
TextButton.Text = "Script Updated execute again(Updated Mod wood)(Click to close)"
TextButton.Font = Enum.Font.SourceSansSemibold
TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
TextButton.TextSize = 14.000
TextButton.MouseButton1Click:connect(function()
TextButton:Remove()
end)
end
